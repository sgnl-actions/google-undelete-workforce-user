  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "eslint": "^9.17.0",
    "jest": "^30.0.5",
    "js-yaml": "^4.1.1",
    "rollup": "^4.28.1"
  }
